# Development defaults for Econ Simulator

# PostgreSQL connection (matches docker-compose.yml)
ECON_SIM_POSTGRES_DSN=postgresql://econ:econ@localhost:5432/econ_simulator
ECON_SIM_POSTGRES_SCHEMA=public
ECON_SIM_POSTGRES_MIN_POOL=1
ECON_SIM_POSTGRES_MAX_POOL=5

# Example Redis URL (optional, leave commented if unused)
ECON_SIM_REDIS_URL=${ECON_SIM_REDIS_URL:-redis://localhost:6379/0}

# Session secret override for local dev (fallback defined in code)
ECON_SIM_SESSION_SECRET=${ECON_SIM_SESSION_SECRET:-econ-sim-session-key}
